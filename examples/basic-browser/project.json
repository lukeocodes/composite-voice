{
  "name": "example-basic-browser",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "examples/basic-browser",
  "projectType": "application",
  "tags": ["type:example", "scope:browser"],
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "dependsOn": ["^build"],
      "options": {
        "command": "vite",
        "cwd": "examples/basic-browser"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "vite build",
        "cwd": "examples/basic-browser"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "vite preview",
        "cwd": "examples/basic-browser"
      }
    }
  },
  "implicitDependencies": ["@lukeocodes/composite-voice"]
}

